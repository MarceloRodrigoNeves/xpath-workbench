<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<VBox spacing="15"
      alignment="CENTER_LEFT"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.m7sistemas.xpathworkbench.ui.XPathFunctionsController">

    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <TableView fx:id="functionTable" prefHeight="400" prefWidth="800">
        <columns>
            <TableColumn fx:id="functionCol" text="Função" prefWidth="150"/>
            <TableColumn fx:id="descriptionCol" text="Descrição" prefWidth="250"/>
            <TableColumn fx:id="modelCol" text="Modelo" prefWidth="200"/>
            <TableColumn fx:id="resultCol" text="Resultado" prefWidth="150"/>
        </columns>
    </TableView>

</VBox>
